# ===========================================
# Apartment Locator AI - Environment Variables
# ===========================================
# Copy this file to .env and fill in your values
# NEVER commit .env to version control

# ===========================================
# Database
# ===========================================
DATABASE_URL="postgresql://user:password@host:5432/database"

# ===========================================
# Authentication
# ===========================================
JWT_SECRET="your-jwt-secret-here"

# ===========================================
# Stripe (Payments)
# ===========================================
STRIPE_SECRET_KEY="sk_test_your_stripe_secret_key"
STRIPE_WEBHOOK_SECRET="whsec_your_webhook_secret"

# Stripe Price IDs (set these to match your Stripe dashboard)
STRIPE_PRICE_RENTER_UNLOCK=""
STRIPE_PRICE_LANDLORD_STARTER_MONTHLY=""
STRIPE_PRICE_LANDLORD_STARTER_ANNUAL=""
STRIPE_PRICE_LANDLORD_PRO_MONTHLY=""
STRIPE_PRICE_LANDLORD_PRO_ANNUAL=""
STRIPE_PRICE_LANDLORD_ENTERPRISE_MONTHLY=""
STRIPE_PRICE_LANDLORD_ENTERPRISE_ANNUAL=""
STRIPE_PRICE_AGENT_BASIC_MONTHLY=""
STRIPE_PRICE_AGENT_BASIC_ANNUAL=""
STRIPE_PRICE_AGENT_TEAM_MONTHLY=""
STRIPE_PRICE_AGENT_TEAM_ANNUAL=""
STRIPE_PRICE_AGENT_BROKERAGE_MONTHLY=""
STRIPE_PRICE_AGENT_BROKERAGE_ANNUAL=""

# ===========================================
# Supabase (Frontend - VITE_ prefix exposes to client)
# ===========================================
VITE_SUPABASE_PROJECT_ID="your-project-id"
VITE_SUPABASE_PUBLISHABLE_KEY="your-supabase-anon-key"
VITE_SUPABASE_URL="https://your-project.supabase.co"
VITE_STRIPE_PUBLISHABLE_KEY="pk_test_your_stripe_publishable_key"

# ===========================================
# AI API
# ===========================================
MOONSHOT_API_KEY="your-moonshot-api-key"

# ===========================================
# Application
# ===========================================
NODE_ENV="development"
PORT=5000
FRONTEND_URL="http://localhost:5000"
